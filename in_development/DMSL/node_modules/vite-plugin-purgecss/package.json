{
  "name": "vite-plugin-purgecss",
  "version": "0.2.12",
  "description": "PurgeCSS plugin for Vite",
  "author": "Cole Crouter",
  "license": "MIT",
  "private": false,
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
