  },
  "devDependencies": {
    "@types/node": "^18.0.3",
    "typescript": "^4.7.4"
  },
  "dependencies": {
    "purgecss": "^4.1.3",
    "tsup": "^6.1.3",
    "vite": "^2.9.14"
